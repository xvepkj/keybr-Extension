{
  "name": "keybr Extension",
  "description": "keybr.com Typing Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "keybr_extension.html",
    "default_icon": "icon.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+K",
        "mac": "MacCtrl+Shift+K"
      },
      "description": "Opens keybr_extension.html"
    }
  }
}